{
  "manifest_version": 2,
  "name": "X-Original-To column",
  "description": "Adds a column for the X-Original-To header to message list panel",
  "version": "0.1",
  "author": "Peter Fabinski",
  "applications": {
    "gecko": {
      "id": "original-to-column@peterfab.com",
      "strict_min_version": "68.0a1",
      "strict_max_version": "91.*"
    }
  },
  "experiment_apis": {
    "FAC": {
      "schema": "schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          ["FAC"]
        ],
        "script": "experiments.js",
        "events": ["startup"]
      }
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
